{
  "private": "true",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vitepress build --base /docs/",
    "build-mpa": "vitepress build --mpa --base /docs/",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "dev": "vitepress",
    "postinstall": "husky install",
    "lint": "prettier --write . && eslint --fix --ext .js,.ts,.vue",
    "lint:check": "pnpm lint:prettier && pnpm lint:eslint && pnpm lint:zh",
    "lint:eslint": "eslint . --ext .js,.ts,.vue",
    "lint:md": "markdownlint **/*.md && lint:zh",
    "lint:prettier": "prettier --check .",
    "lint:zh": "zhlint 'docs/**/*.md'",
    "serve": "vitepress serve --base /docs/",
    "site:preview": "vitepress build --outDir _dist --base /",
    "up": "taze major -I"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,sass,css}": [
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "prettier": "prettier-config-vuepress",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@iconify-json/ic": "^1.1.17",
    "@iconify-json/logos": "^1.1.42",
    "@iconify-json/mdi": "^1.1.66",
    "@iconify-json/ph": "^1.1.13",
    "@mdit/plugin-container": "^0.1.0-alpha.1",
    "@mdit/plugin-figure": "^0.12.0",
    "@mdit/plugin-img-mark": "^0.12.0",
    "@mdit/plugin-img-size": "^0.12.0",
    "@mdit/plugin-mark": "^0.12.0",
    "@mdit/plugin-sub": "^0.12.0",
    "@mdit/plugin-sup": "^0.12.0",
    "@types/js-yaml": "^4.0.9",
    "@vueuse/core": "^10.10.0",
    "@vueuse/integrations": "^10.10.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "feed": "^4.2.2",
    "js-yaml": "^4.1.0",
    "ky": "^1.3.0",
    "markdown-it-footnote": "^4.0.0",
    "medium-zoom": "^1.1.0",
    "pinia": "^2.1.7",
    "qrcode": "^1.5.3",
    "sass": "^1.77.4",
    "typescript": "^5.4.5",
    "ua-parser-js": "^1.0.38",
    "unocss": "^0.60.4",
    "vitepress": "1.2.3",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@nolebase/markdown-it-bi-directional-links": "^2.1.1",
    "@nolebase/markdown-it-unlazy-img": "^2.1.1",
    "@nolebase/vitepress-plugin-git-changelog": "^2.1.0",
    "@nolebase/vitepress-plugin-highlight-targeted-heading": "^2.1.0",
    "@nolebase/vitepress-plugin-inline-link-preview": "^2.1.1",
    "@nolebase/vitepress-plugin-thumbnail-hash": "^2.1.1",
    "@types/eslint": "^8.56.10",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.14.2",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@unocss/eslint-config": "^0.60.4",
    "@unocss/extractor-mdc": "^0.60.4",
    "@vite-pwa/vitepress": "^0.5.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.4.0",
    "eslint-config-vuepress": "^4.10.1",
    "eslint-config-vuepress-typescript": "^4.10.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "9.16.1",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-kbd-better": "^1.2.5",
    "markdownlint-cli": "^0.41.0",
    "pnpm": "^9.1.4",
    "prettier": "^3.3.0",
    "prettier-config-vuepress": "^4.4.0",
    "sort-package-json": "^2.10.0",
    "stylelint": "^16.6.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "taze": "^0.13.8",
    "tsconfig-vuepress": "^4.5.0",
    "vite-plugin-inspect": "^0.8.4",
    "zhlint": "^0.8.1"
  },
  "packageManager": "pnpm@9.1.4"
}
