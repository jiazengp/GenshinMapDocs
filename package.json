{
  "name": "yuanshenmap-docs",
  "version": "2.1.1",
  "private": true,
  "description": "yuanshenmap-docs",
  "homepage": "https://yuanshen.site/docs/",
  "bugs": {
    "url": "https://github.com/jiazengp/genshinmap-docs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiazengp/genshinmap-docs.git",
    "branch": "master"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/genshinmap"
  },
  "license": "MIT",
  "author": " <jiazengp@gmail.com> (^_^)",
  "directories": {
    "doc": "docs"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc --build",
    "commit": "npx git-cz",
    "dev": "lerna run dev --parallel",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "release-it --no-git.requireCleanWorkingDir",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:clean": "lerna clean",
    "lerna:list": "lerna list",
    "docs:dev": "vuepress dev docs --clean-cache",
    "docs:clean": "rimraf docs/.vuepress/.temp docs/.vuepress/.cache ./dist",
    "docs:build": "vuepress build docs --clean-cache",
    "docs:build2": "vuepress build docs",
    "docs:serve": "npx http-server ./dist -b",
    "docs:info": "vuepress info",
    "lint": "yarn lint:prettier --write && yarn lint:eslint --fix",
    "lint:check": "yarn lint:prettier && yarn lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts,.vue",
    "lint:md": "yarn lint:md-lint && yarn lint:md-remark && yarn lint:md-textlint",
    "lint:md-lint": "markdownlint **/*.md",
    "lint:md-remark": "remark --quiet .",
    "lint:md-textlint": "textlint ./docs/*.md",
    "lint:prettier": "prettier --check .",
    "test": "jest -i",
    "test:ui": "karma start",
    "test:coverage": "jest --coverage",
    "preview": "vite preview --port 8080 --host",
    "preview-https": "serve dist",
    "packages:check-update": "ncu --deep --timeout 600000",
    "packages:update": "ncu -u --deep --timeout 600000 && yarn install && yarn upgrade",
    "packages:doctor": "ncu --doctor -u",
    "packages:taze": "npx taze -r",
    "packages:major": "npm taze major",
    "packages:minor": "npx taze minor",
    "packages:sort": "sort-package-json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      "> 5%"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "prettier-config-vuepress",
  "remarkConfig": {
    "plugins": [
      "frontmatter",
      [
        "footnotes",
        {
          "inlineNotes": true
        }
      ],
      "math",
      "preset-lint-consistent",
      "preset-lint-recommended",
      "preset-lint-markdown-style-guide",
      [
        "lint-maximum-line-length",
        false
      ],
      [
        "lint-list-item-spacing",
        false
      ],
      [
        "lint-list-item-indent",
        "space"
      ],
      [
        "lint-emphasis-marker",
        false
      ]
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@textlint-rule/textlint-rule-no-unmatched-pair": "^1.0.8",
    "@types/concurrently": "^6.2.0",
    "@types/eslint": "^7.2.13",
    "@types/http-server": "^0.12.0",
    "@types/jest": "^26.0.23",
    "@types/katex": "^0.11.0",
    "@types/markdown-it": "^12.0.2",
    "@types/node": "^15.12.5",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.4.1",
    "@types/qs": "^6.9.6",
    "@types/semver": "^7.3.6",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "@vuepress/plugin-debug": "^2.0.0-beta.19",
    "@vuepress/plugin-git": "^2.0.0-beta.20",
    "@vuepress/plugin-google-analytics": "^2.0.0-beta.19",
    "@vuepress/plugin-pwa": "^2.0.0-beta.19",
    "@vuepress/plugin-pwa-popup": "^2.0.0-beta.19",
    "@vuepress/plugin-register-components": "^2.0.0-beta.19",
    "@vuepress/plugin-search": "^2.0.0-beta.19",
    "@vuepress/plugin-shiki": "^2.0.0-beta.19",
    "@vuepress/plugin-toc": "^2.0.0-beta.19",
    "@vueuse/core": "^5.0.3",
    "axios": "^0.21.1",
    "concurrently": "^6.2.0",
    "conventional-changelog-cli": "^2.1.1",
    "cpx2": "^3.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.10.5",
    "default-passive-events": "^2.0.0",
    "dotenv": "^10.0.0",
    "element-plus": "^1.0.2-beta.53",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-vuepress": "^3.2.1",
    "eslint-config-vuepress-typescript": "^2.2.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "https-localhost": "^4.6.5",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "jest-serializer-vue": "^2.0.2",
    "js-md5": "^0.7.3",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^5.0.0",
    "katex": "^0.13.11",
    "lerna": "^4.0.0",
    "lint-md": "^0.2.0",
    "lint-staged": "^11.0.0",
    "markdownlint-cli": "0.28.1",
    "mocha": "^9.0.1",
    "npm-check-updates": "^11.7.1",
    "prettier": "^2.3.2",
    "prettier-config-vuepress": "^1.2.0",
    "qrcode": "^1.4.4",
    "qs": "^6.10.1",
    "release-it": "^14.10.0",
    "screenfull": "^5.1.0",
    "semantic-release": "^17.4.4",
    "semver": "^7.3.5",
    "sort-package-json": "^1.50.0",
    "textlint": "^12.0.0",
    "textlint-filter-rule-allowlist": "^3.0.0",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-alex": "^3.0.0",
    "textlint-rule-apostrophe": "^2.0.0",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-date-weekday-mismatch": "^1.0.5",
    "textlint-rule-en-capitalization": "^2.0.3",
    "textlint-rule-en-spell": "^1.2.3",
    "textlint-rule-ja-no-abusage": "^3.0.0",
    "textlint-rule-ja-no-mixed-period": "^2.1.1",
    "textlint-rule-ja-space-between-half-and-full-width": "^2.1.3",
    "textlint-rule-no-dead-link": "^4.7.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "textlint-rule-stop-words": "^2.0.9",
    "textlint-rule-terminology": "^2.1.5",
    "textlint-rule-write-good": "^2.0.0",
    "textlint-rule-zh-half-and-full-width-bracket": "^1.1.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "vuepress": "^2.0.0-beta.22",
    "vuepress-plugin-feed2": "^2.0.0-alpha.5",
    "vuepress-plugin-lightgallery": "^2.0.0-alpha.4",
    "vuepress-plugin-md-enhance": "^2.0.0-alpha.5",
    "vuepress-plugin-photo-swipe": "^2.0.0-alpha.5",
    "vuepress-plugin-sass-palette": "^2.0.0-alpha.5",
    "vuepress-plugin-seo2": "^2.0.0-alpha.5",
    "vuepress-plugin-sitemap2": "^2.0.0-alpha.5",
    "vuepress-vite": "^2.0.0-beta.18",
    "yaml": "^2.0.0-6"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
